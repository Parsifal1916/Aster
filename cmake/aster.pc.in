prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/@CMAKE_INSTALL_LIBDIR@
includedir=${prefix}/@CMAKE_INSTALL_INCLUDEDIR@

Name: Aster
Description: Aster Graphics Library
Version: @PROJECT_VERSION@
Libs: -L${libdir} -lAster -ltbb
Libs.private: -lGL -lGLEW -lglfw -lOpenCL -lpthread -ldl
Cflags: -I${includedir}